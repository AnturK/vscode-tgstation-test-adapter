{
  "compilerOptions": {
    "target": "es2016",
    "lib": [ "es2016" ],
    "module": "commonjs",
    "outDir": "out",
    "importHelpers": true,
    "sourceMap": true,
    "strict": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "removeComments": true,
    "skipLibCheck": true,
    "typeRoots": [ "typings", "node_modules/@types"]
  },
  "include": [
    "src/main.ts",
    "@types/*"
  ],
  "exclude": [
      "node_modules",
      "typings"
  ]
}
  